<Window x:Class="BudloApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BudloApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#FF485E61">
        <Button Content="Workouts" HorizontalAlignment="Left" Margin="640,105,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Diets" HorizontalAlignment="Left" Margin="640,160,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Tips" HorizontalAlignment="Left" Margin="640,223,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Member" HorizontalAlignment="Left" Margin="640,281,0,0" VerticalAlignment="Top" Width="75"  Command="{Binding Path=SaveCommand}" />
        <Button Content="Register" HorizontalAlignment="Left" Margin="640,338,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-7.027,-2.711" />
    </Grid>
</Window>
